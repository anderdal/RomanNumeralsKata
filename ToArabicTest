using RomanToArabicController

[TestClass]
public void MTest()
{
  //Arrange
  RomanToArabicController controller = new RomanToArabicController();
  string numeral = "M";
  
  //Act
  var result = controller.ToArabic(numeral);
  
  //Assert
  Assert.IsTrue(result, 1000)
}
